CREATE TABLE ALUNO (
	MATRICULA INT PRIMARY KEY,
	NOME CHAR(50),
	EXEMPLARESLOCADOS SMALLINT,
	NOMECURSO CHAR(50)
);
CREATE TABLE PROFESSOR (
	MATRICULA INT PRIMARY KEY,
	TITULO CHAR(50),
	EXEMPLARESLOCADOS SMALLINT
);

CREATE TABLE CURSO (
	ID INT PRIMARY KEY,
	NOMECURSO CHAR(50)
);
CREATE TABLE LIVRO (
	CODIGO INT PRIMARY KEY,
	NOME CHAR(30),
	AUTOR CHAR(50)
);

CREATE TABLE EXEMPLAR (
	CODIGO INT PRIMARY KEY,
	LOCALIZACAO CHAR(60),
	EDICAO CHAR(30),
	STATUS BOOLEAN
);
